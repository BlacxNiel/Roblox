local jobId = game.JobId
print("JOB ID DEL SERVIDOR: " .. jobId)

if setclipboard then
    setclipboard(jobId)
    print("✔ JobId copiado al portapapeles!")
else
    print("⚠ Tu ejecutor no soporta setclipboard.")
end
game.StarterGui:SetCore("SendNotification", {
    Title = "JOB ID";
    Text = jobId;
    Duration = 10;
})
